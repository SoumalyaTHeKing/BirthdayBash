{
  "name": "birthday-bash",
  "version": "1.0.0",
  "description": "chetna's birthday gift",
  "main": "index.html",
  "scripts": {
    "start": "npm run lite",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lite": "lite-server",
    "clean": "rimraf dist",
    "imagemin": "imagemin /* -o dist/img",
    "usemin": "usemin gallery.html -d dist --htmlmin -o dist/gallery.html && usemin video.html -d dist --htmlmin -o dist/video.html && usemin index.html -d dist --htmlmin -o dist/index.html",
    "build": "npm run clean && npm run imagemin && npm run usemin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SoumalyaTHeKing/BirthdayBash.git"
  },
