  },
  "homepage": "https://github.com/SoumalyaTHeKing/BirthdayBash#readme",
  "devDependencies": {
    "cssmin": "^0.4.3",
    "htmlmin": "0.0.7",
    "lite-server": "^2.5.4",
    "rimraf": "^2.6.2",
    "uglify-js": "^2.4.11",
    "usemin-cli": "^0.5.1"
  },
  "dependencies": {
    "bootstrap": "^4.5.0",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1",
    "typed.js": "^2.0.11"
